@using Microsoft.AspNetCore.Http
@model AzureDevOpsDemoBuilder.Models.ProjectList.ProjectDetails
@{
    ViewBag.Title = "Azure DevOps Demo Generator - Template Builder";
}

<!DOCTYPE html>
<html lang="en">
